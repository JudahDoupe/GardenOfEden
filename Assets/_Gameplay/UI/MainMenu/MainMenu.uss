#Left {
    transition-duration: 0.3s;
    transition-timing-function: ease-out;
    translate: 0.1% 0;
}

#Left.Hidden {
    translate: -100% 0;
}

#Right {
    transition-duration: 0.3s;
    transition-timing-function: ease-out;
    translate: 0.1% 0;
}

#Right.Hidden {
    translate: 100% 0;
}

#Header {
    transition-duration: 0.3s;
    transition-timing-function: ease-out;
    translate: 0 0.1%;
}

#Header.Hidden {
    translate: 0 -100%;
}

#Footer {
    transition-duration: 0.3s;
    transition-timing-function: ease-out;
    translate: 0 0.1%;
}

#Footer.Hidden {
    translate: 0 100%;
}

#NewPlanetInput > Label {
    -unity-text-align: upper-center;
    font-size: 30px;
    color: rgb(255, 255, 255);
    -unity-font-definition: none;
    -unity-font: url('project://database/Assets/_Gameplay/UI/Common/Fonts/Raleway/Raleway-Light.ttf?fileID=12800000&guid=1e4724edc335041458658f2bc32fabd8&type=3#Raleway-Light');
}

#NewPlanetInput > TextInput {
    font-size: 24px;
    width: 100%;
    -unity-text-align: upper-center;
    margin-left: 25px;
    margin-right: 25px;
    margin-top: 25px;
    margin-bottom: 25px;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    background-color: rgba(0, 0, 0, 0.7);
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    border-left-color: rgba(255, 255, 255, 0.7);
    border-right-color: rgba(255, 255, 255, 0.7);
    border-top-color: rgba(255, 255, 255, 0.7);
    border-bottom-color: rgba(255, 255, 255, 0.7);
    color: rgb(255, 255, 255);
    -unity-font-definition: none;
    -unity-font: url('project://database/Assets/_Gameplay/UI/Common/Fonts/Raleway/Raleway-Bold.ttf?fileID=12800000&guid=dbe3c3546677f8f4cbb54d830e9a59f5&type=3#Raleway-Bold');
}

#NewPlanetInput.Error > TextInput {
    border-left-color: rgba(255, 0, 0, 0.7);
    border-right-color: rgba(255, 0, 0, 0.7);
    border-top-color: rgba(255, 0, 0, 0.7);
    border-bottom-color: rgba(255, 0, 0, 0.7);
}

.Popup {
    background-color: rgba(0, 0, 0, 0.7);
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    transition-duration: 0.3s;
}

.Popup.Hidden {
    display: none;
    scale: 0.001 0.001;
}
