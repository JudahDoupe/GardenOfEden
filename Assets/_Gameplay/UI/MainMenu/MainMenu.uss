#Continue .Icon {
    background-image: url('project://database/Assets/_Gameplay/UI/MainMenu/Icons/play-button-arrowhead.png?fileID=2800000&guid=935e65856c94c65428918d4972c46452&type=3#play-button-arrowhead');
