#Left {
    transition-duration: 0.3s;
    transition-timing-function: ease-out;
    translate: 0 0;
}

#Left.Hidden {
    translate: -100% 0;
}

#Right {
    transition-duration: 0.3s;
    transition-timing-function: ease-out;
    translate: 0 0;
}

#Right.Hidden {
    translate: 100% 0;
}

#Header {
    transition-duration: 0.3s;
    transition-timing-function: ease-out;
    translate: 0 0;
}

#Header.Hidden {
    translate: 0 -100%;
}

#Footer {
    transition-duration: 0.3s;
    transition-timing-function: ease-out;
    translate: 0 0;
}

#Footer.Hidden {
    translate: 0 100%;
}
