.Button {
    width: 60px;
    height: 60px;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    background-color: rgba(0, 0, 0, 0.7);
    margin-left: 5px;
    margin-right: 5px;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    border-left-color: rgba(255, 255, 255, 0.7);
    border-right-color: rgba(255, 255, 255, 0.7);
    border-top-color: rgba(255, 255, 255, 0.7);
    border-bottom-color: rgba(255, 255, 255, 0.7);
    transition-duration: 0.1s;
    transition-timing-function: ease-out;
    overflow: hidden;
}

.Button:hover {
    -unity-background-image-tint-color: rgb(30, 30, 30);
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    scale: 1.1 1.1;
}

.Button:hover > .Icon {
    -unity-background-image-tint-color: rgb(255, 255, 255);
}

.Button > .Icon {
    -unity-background-image-tint-color: rgb(175, 175, 175);
}

.Icon {
    flex-grow: 1;
    flex-shrink: 0;
}

.Title {
    -unity-font: url('project://database/Assets/_Gameplay/UI/Common/Fonts/Raleway/Raleway-Bold.ttf?fileID=12800000&guid=dbe3c3546677f8f4cbb54d830e9a59f5&type=3#Raleway-Bold');
    -unity-font-definition: none;
    font-size: 40px;
    background-color: rgba(0, 0, 0, 0.7);
    width: auto;
    -unity-text-align: middle-center;
    height: 60px;
    min-width: 400px;
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    color: rgb(255, 255, 255);
}

